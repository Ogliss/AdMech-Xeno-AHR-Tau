    <HediffDef ParentName="AddedBodyPartBase">
    <defName>OG_Kroot_Mutation_HyperactiveNymuneOrgan</defName>
    <label>hyperactive</label>
    <labelNoun>a hyperactive nymune organ</labelNoun>
    <description>description</description>
    <spawnThingOnRemoved>OG_Kroot_Mutation_HyperactiveNymuneOrgan</spawnThingOnRemoved>
    <addedPartProps>
      <solid>false</solid>
      <betterThanNatural>true</betterThanNatural>
      <partEfficiency>1.15</partEfficiency>
    </addedPartProps>
    <stages>
      <li>
        <statOffsets>
          <ResearchSpeed>0.2</ResearchSpeed>
        </statOffsets>          
        <foodPoisoningChanceFactor>0.25</foodPoisoningChanceFactor>
        <restFallFactorOffset>-0.1</restFallFactorOffset>
      </li>
    </stages>
  </HediffDef>
  
