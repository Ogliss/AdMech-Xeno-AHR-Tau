<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- ============================ Gue'vesa Base ========================== -->
  <PawnKindDef Name="OG_Guevesa_Base_KindDef" Abstract="True">
    <defaultFactionType>OG_Tau_PlayerColony</defaultFactionType>
    <chemicalAddictionChance>0.035</chemicalAddictionChance>
    <baseRecruitDifficulty>1</baseRecruitDifficulty>
    <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
  </PawnKindDef>

  <!-- ============================ Gue'vesa Civilian Base ========================== -->
  <PawnKindDef Name="OG_Guevesa_Civ_Base" ParentName="OG_Guevesa_Base_KindDef" Abstract="True">
    <combatPower>25</combatPower>
	<race>Human</race>
    <baseRecruitDifficulty>0.45</baseRecruitDifficulty>
    <itemQuality>Normal</itemQuality>
    <isFighter>false</isFighter>
    <gearHealthRange>
      <min>0.2</min>
      <max>2.0</max>
    </gearHealthRange>
    <apparelMoney>
      <min>300</min>
      <max>900</max>
    </apparelMoney>
    <apparelTags>
      <li>Outlander</li>
    </apparelTags>
    <weaponMoney>
      <min>100</min>
      <max>750</max>
    </weaponMoney>
    <weaponTags>
		<li>TBasicGun</li>
     	<li>NeolithicMeleeBasic</li>
    </weaponTags>
    <techHediffsMoney>
      <min>200</min>
      <max>300</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
    <techHediffsChance>0.51</techHediffsChance>
    <invNutrition>2.55</invNutrition>
    <inventoryOptions>
      <skipChance>0.75</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>90</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>25</min>
            <max>70</max>
          </countRange>
        </li>
        <li>
          <choiceChance>10</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>700</min>
            <max>1000</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
    <modExtensions>
		<li MayRequire="CETeam.CombatExtended" Class="CombatExtended.LoadoutPropertiesExtension">
			<primaryMagazineCount>
			  <min>2</min>
			  <max>5</max>
			</primaryMagazineCount>
		</li>
    </modExtensions>
  </PawnKindDef>
  
  <!-- ============================ Gue'vesa Civilians ========================== -->
  <!-- ============================ Gue'vesa Faction Leader ========================== -->
  <PawnKindDef ParentName="OG_Guevesa_Civ_Base">
    <defName>OG_Guevesa_Commander_Faction</defName>
    <label>Commander</label>
    <labelPlural>Commanders</labelPlural>
    <factionLeader>true</factionLeader>
    <isFighter>true</isFighter>
    <gearHealthRange>
      <min>0.5</min>
      <max>2.0</max>
    </gearHealthRange>
    <apparelMoney>
      <min>1000</min>
      <max>5000</max>
    </apparelMoney>
    <apparelTags>
      <li>Tribal</li>
      <li>Neolithic</li>
    </apparelTags>
    <weaponMoney>
      <min>10000</min>
      <max>15500</max>
    </weaponMoney>
    <weaponTags>
		<li>THonourBlade</li>
    </weaponTags>
    <techHediffsMoney>
      <min>200</min>
      <max>300</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>ImperialFighter</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
	<minGenerationAge>60</minGenerationAge>
	<maxGenerationAge>600</maxGenerationAge>
  </PawnKindDef>

  <!-- ============================ Gue'vesa Trader ========================== -->
  <PawnKindDef ParentName="OG_Guevesa_Civ_Base">
    <defName>OG_Guevesa_Trader</defName>
    <label>Gue'vesa trader</label>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>Outlander</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
  </PawnKindDef>  
  
  <!-- ============================ Gue'vesa Slave ========================== -->
  <PawnKindDef ParentName="OG_Guevesa_Civ_Base">
    <defName>Slave_OG_Guevesa</defName>
    <label>Gue'vesa slave</label>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>Pirate</li>
          <li>ImperialFighter</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
  </PawnKindDef>
    
  <!-- ============================ Gue'vesa Security ========================== -->
