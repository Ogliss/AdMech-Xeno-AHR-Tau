      <max>1500</max>
    </apparelMoney>
    <apparelTags>
      <li>Outlander</li>
      <li>Military</li>
      <li>TauMilitary</li>
    </apparelTags>
    <weaponMoney>
      <min>200</min>
      <max>1500</max>
    </weaponMoney>
    <weaponTags>
	
    </weaponTags>
    <techHediffsMoney>
      <min>200</min>
      <max>500</max>
    </techHediffsMoney>
    <techHediffsTags>
      <li>Simple</li>
    </techHediffsTags>
    <techHediffsChance>0.20</techHediffsChance>
    <invNutrition>2.55</invNutrition>
    <inventoryOptions>
      <skipChance>0.95</skipChance>
      <thingDef>Silver</thingDef>
      <countRange>
        <min>30</min>
        <max>100</max>
      </countRange>
    </inventoryOptions>
  </PawnKindDef>

  <!-- ============================ Gue'vesa Military Base ========================== -->
  <PawnKindDef Name="OG_Guevesa_Mil_Base" ParentName="OG_Guevesa_Base_KindDef" Abstract="True">
    <chemicalAddictionChance>0</chemicalAddictionChance>
    <isFighter>true</isFighter>
