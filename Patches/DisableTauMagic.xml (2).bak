<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!--Tau-->
	<!-- stops tau getting magical abilities from A Rimworld of Magic -->
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationConditional">
				<xpath>
					/Defs/TraitDef[defName = "Gifted"]
				</xpath>
				<success>Always</success>
				<match Class="PatchOperationAdd">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace [defName = "Alien_Tau"]/alienRace/generalSettings/disallowedTraits</xpath>
					<value>
					<li>Gifted</li>
					<li>InnerFire</li>
					<li>HeartOfFrost</li>
					<li>StormBorn</li>
					<li>Arcanist</li>
					<li>Druid</li>
					<li>Paladin</li>
					<li>Summoner</li>
					<li>Necromancer</li>
					<li>Lich</li>
					<li>Priest</li>
					<li>Geomancer</li>
					<li>Technomancer</li>
					<li>Succubus</li>
					<li>Warlock</li>
					<li>TM_Psionic</li>
					<li>DeathKnight</li>
					</value>
				</match>
			</li>
		</operations>
	</Operation>
	
